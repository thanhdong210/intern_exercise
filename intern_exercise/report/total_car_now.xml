<odoo>
    <template id="parking_lot_report">
        <t t-foreach="docs" t-as="o">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>
                            <div t-field="o.parking_lot_name"/>
                            <span>Number of car now: </span>
                            <span t-field="o.total_car_now"/>
                            <span t-field="lines.parking_ticket_name"/>
                        </h2>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>